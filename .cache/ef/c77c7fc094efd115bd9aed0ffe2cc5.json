inal":{"line":40,"column":3},"generated":{"line":99,"column":2}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":40,"column":3},"generated":{"line":100,"column":0}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":42},"generated":{"line":100,"column":2}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":42},"generated":{"line":100,"column":8}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":42},"generated":{"line":100,"column":9}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":46},"generated":{"line":100,"column":16}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":0},"generated":{"line":101,"column":0}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":0},"generated":{"line":101,"column":7}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":0},"generated":{"line":101,"column":8}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":0},"generated":{"line":101,"column":15}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":0},"generated":{"line":101,"column":18}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":0},"generated":{"line":101,"column":30}}],"sources":{"src/page/news-feed-view.ts":"import View from '../core/view';\r\nimport { NewsFeedApi } from '../core/api';\r\nimport { NewsStore, NewsFeed } from '../types';\r\nimport { NEWS_URL } from '../config';\r\n\r\nconst template = `\r\n<div class=\"bg-gray-600 min-h-screen\">\r\n  <div class=\"bg-white text-xl\">\r\n    <div class=\"mx-auto px-4\">\r\n      <div class=\"flex justify-between items-center py-6\">\r\n        <div class=\"flex justify-start\">\r\n          <h1 class=\"font-extrabold\">Hacker News</h1>\r\n        </div>\r\n        <div class=\"items-center justify-end\">\r\n          <a href=\"#/page/{{__prev_page__}}\" class=\"text-gray-500\">\r\n            Previous\r\n          </a>\r\n          <a href=\"#/page/{{__next_page__}}\" class=\"text-gray-500 ml-4\">\r\n            Next\r\n          </a>\r\n        </div>\r\n      </div> \r\n    </div>\r\n  </div>\r\n  <div class=\"p-4 text-2xl text-gray-700\">\r\n    {{__news_feed__}}        \r\n  </div>\r\n</div>\r\n`;\r\n\r\nexport default class NewsFeedView extends View {\r\n  private api: NewsFeedApi;\r\n  private store: NewsStore;\r\n\r\n  constructor(containerId: string, store: NewsStore) {\r\n    super(containerId, template);\r\n\r\n    this.store = store;\r\n    this.api = new NewsFeedApi(NEWS_URL);\r\n  }\r\n\r\n  render = async (page: string = '1'): Promise<void> => {\r\n    this.store.currentPage = Number(page);\r\n\r\n    if (!this.store.hasFeeds) {\r\n      this.store.setFeeds(await this.api.getData());\r\n    }  \r\n\r\n    for(let i = (this.store.currentPage - 1) * 10; i < this.store.currentPage * 10; i++) {\r\n      const { id, title, comments_count, user, points, time_ago, read } = this.store.getFeed(i);\r\n\r\n      this.addHtml(`\r\n        <div class=\"p-6 ${read ? 'bg-red-500' : 'bg-white'} mt-6 rounded-lg shadow-md transition-colors duration-500 hover:bg-green-100\">\r\n          <div class=\"flex\">\r\n            <div class=\"flex-auto\">\r\n              <a href=\"#/show/${id}\">${title}</a>  \r\n            </div>\r\n            <div class=\"text-center text-sm\">\r\n              <div class=\"w-10 text-white bg-green-300 rounded-lg px-0 py-2\">${comments_count}</div>\r\n            </div>\r\n          </div>\r\n          <div class=\"flex mt-3\">\r\n            <div class=\"grid grid-cols-3 text-sm text-gray-500\">\r\n              <div><i class=\"fas fa-user mr-1\"></i>${user}</div>\r\n              <div><i class=\"fas fa-heart mr-1\"></i>${points}</div>\r\n              <div><i class=\"far fa-clock mr-1\"></i>${time_ago}</div>\r\n            </div>  \r\n          </div>\r\n        </div>    \r\n      `);\r\n    } \r\n   \r\n\r\n    this.setTemplateData('news_feed', this.getHtml());\r\n    this.setTemplateData('prev_page', String(this.store.prevPage));\r\n    this.setTemplateData('next_page', String(this.store.nextPage));\r\n  \r\n    this.updateView();\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"51ac1bfb7f338171aa8ae19d90b3c700","cacheData":{"env":{}}}